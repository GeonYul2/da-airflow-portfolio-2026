AIRFLOW_UID=50000
AIRFLOW_GID=0

# Local dev credentials/secrets (change for non-local use)
AIRFLOW_WWW_USER_PASSWORD=admin
POSTGRES_PASSWORD=postgres
MYSQL_ROOT_PASSWORD=root
MYSQL_PASSWORD=airflow
AIRFLOW_API_SECRET_KEY=local-dev-secret-local-dev-secret-local-dev-secret-local-dev-secret
AIRFLOW_API_AUTH_JWT_SECRET=local-dev-jwt-secret-local-dev-jwt-secret-local-dev-jwt-secret-local-dev-jwt-secret

# Default warehouse (Postgres)
# WAREHOUSE_DSN=postgresql://airflow:airflow@postgres:5432/warehouse

# MySQL/MariaDB warehouse mode (set both together)
# WAREHOUSE_DSN=mysql+pymysql://airflow:airflow@mariadb:3306/warehouse
# SQL_ROOT=sql/mysql

# Optional
# RAW_FRESHNESS_MAX_HOURS=168
